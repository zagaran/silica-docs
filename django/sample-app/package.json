{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"yarn dev:vue\" \"yarn dev:static\"",
    "dev:vue": "vite build --watch --config vite-vue.config.js --mode dev",
    "dev:static": "vite build --watch --config vite-static.config.js --mode dev",
    "build": "yarn build:vue && yarn build:static && yarn es-check",
    "build:vue": "vite build --config vite-vue.config.js",
    "build:static": "vite build --config vite-static.config.js"
  },
  "dependencies": {
    "bootstrap": "^5.2.1",
    "silica-vue": "^0.1.0-alpha.9",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@vitejs/plugin-vue2": "^2.0.0",
    "concurrently": "^7.1.0",
    "glob": "^8.0.3",
    "rollup-plugin-external-globals": "^0.6.1",
    "terser": "^5.15.0",
    "vite": "^3.1.1",
    "vue": "^2.7.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ]
}
