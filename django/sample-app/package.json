{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"yarn dev:vue\" \"yarn dev:static\"",
    "dev:vue": "vue-cli-service serve",
    "dev:static": "WEBPACK_ENV=dev webpack --watch",
    "build": "yarn build:vue && yarn build:static && yarn es-check",
    "build:vue": "vue-cli-service build",
    "build:static": "WEBPACK_ENV=build webpack",
    "es-check": "es-check es5 './static/js/dist/vue/**/*-legacy.js' && es-check es5 './static/js/dist/!(vue)**/*.js'"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "vue-fragment": "^1.5.2"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "@vue/composition-api": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "acorn": "^8.7.1",
    "babel-loader": "^8.2.5",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^7.1.0",
    "core-js": "^3.8.3",
    "es-check": "^6.2.1",
    "glob": "^8.0.1",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ]
}
